{
  "name": "@useblu/ocean-icons",
  "version": "0.0.0",
  "private": "true",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "run-p build:*",
    "build:outline": "svgo --config=svgo.outline.yaml -f ./src/outline -o ./dist/outline -q",
    "build:solid": "svgo --config=svgo.solid.yaml -f ./src/solid -o ./dist/solid -q"
  },
  "devDependencies": {
    "svgo": "^2.0.0"
  }
}
